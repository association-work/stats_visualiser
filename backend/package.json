{
  "name": "@wikidata/backend",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "build": "tsc",
    "tsc:noEmit": "tsc --noEmit",
    "start": "ts-node --files -r tsconfig-paths/register src/server/index.ts",
    "start:js": "node -r tsconfig-paths/register --enable-source-maps src/server/index.js",
    "test": "jest --verbose",
    "dotenv": "dotenv",
    "job:geo": "ts-node --files -r tsconfig-paths/register src/scripts/geo.ts",
    "job:ges": "ts-node --files -r tsconfig-paths/register src/scripts/ges.ts",
    "job:pop": "ts-node --files -r tsconfig-paths/register src/scripts/population.ts",
    "job:setup": "ts-node --files -r tsconfig-paths/register src/scripts/setup.ts",
    "db:test-migrate": "dotenv -e .env.test -- npx prisma migrate dev  --schema ./src/database/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "body-parser": "~2.2.0",
    "cors": "^2.8.5",
    "dotenv": "~17.2.1",
    "express": "^5.1.0",
    "helmet": "~8.1.0",
    "inversify": "~7.2.0",
    "pg": "~8.16.3",
    "prisma": "~6.14.0",
    "reflect-metadata": "0.2.2",
    "tsconfig-paths": "^4.2.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "~22.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "~5.0.3",
    "@types/jest": "~29.5.14",
    "@types/node": "~22.13.10",
    "@types/pg": "~8.15.5",
    "jest": "~29.7.0",
    "ts-jest": "~29.2.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "dotenv-cli": "~10.0.0"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
